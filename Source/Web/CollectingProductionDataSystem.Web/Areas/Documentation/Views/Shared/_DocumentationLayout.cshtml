                            <li class="list-group-item mylist-group-item">
                                @Ajax.ActionLink("Сменни данни за отчети", "ShiftData", "Help",
                                             new AjaxOptions() { HttpMethod = "Get", InsertionMode = InsertionMode.Replace, UpdateTargetId = "mypage-content", LoadingElementId = "inprogress" })
                            </li>
                            <li class="list-group-item mylist-group-item">
                                @Ajax.ActionLink("Сменни данни за измервателни позиции", "ShiftDataMeasuringPosition", "Help",
