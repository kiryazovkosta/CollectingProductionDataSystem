</aside>
<article class="col-sm-10 documentation">
    <div id="inprogress" style="display:none;">
        <img src="~/Content/Images/loading.gif" />
    </div>
    <div id="mypage-content" class="container-fluid">
        @RenderBody()
    </div>
</article>

@section scripts{
    @Scripts.Render("~/Scripts/jquery.unobtrusive-ajax.min.js")
    <script src="~/Scripts/custom/documentation.js"></script>
    @RenderSection("scripts", false);
}


