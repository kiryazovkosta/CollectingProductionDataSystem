@model IEnumerable<CollectingProductionDataSystem.Models.InventoryTank>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    @foreach(var tank in Model)
    {
        <p>@tank.ControlPoint</p>
        <p>@tank.TankName</p>
        <p>@tank.PhdTagAvailableRoom</p>
        <p>@tank.PhdTagFreeWaterLevel</p>
        <p>@tank.PhdTagProductLevel</p>
        <p>@tank.PhdTagWeightInAir</p>
        <p>@tank.PhdTagNetStandardVolume</p>
    }
</div>